<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout
        android:id="@+id/phonetool"
        android:layout_width="match_parent"
        android:layout_height="50.0dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:background="@drawable/gengduo_modifybiaoti12">
        <TextView
            android:id="@+id/phonetool_text1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.6"
            android:paddingLeft="20dp"
            android:textColor="#ffffff"
            android:text="@string/more_back"
            android:gravity="center_vertical"
            android:textSize="16sp"/>
        <ImageView
            android:id="@+id/phonetool_text2"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:layout_marginRight="100dp"
            android:layout_weight="1"
            android:src="@mipmap/lianxidianhua"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/contact_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp"
        android:background="@drawable/roundshape"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/contact_main_1"
            android:layout_width="match_parent"
            android:layout_height="45.0dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/contact_main_1_left"
                android:layout_width="80dp"
                android:layout_height="45.0dp"
                android:gravity="center_vertical|center_horizontal">
                <TextView
                    android:id="@+id/contact_main_1_left_text"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="联系人1："
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="#666666"
                    android:textSize="15sp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/contact_main_1_right"
                android:layout_width="match_parent"
                android:layout_height="45.0dp"
                android:gravity="center_vertical|center_horizontal">
                <TextView
                    android:id="@+id/contact_main_1_right_text"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="18332725865"
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="#666666"
                    android:textSize="18sp"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/contact_main_1_image"
                android:layout_width="45dp"
                android:layout_height="45.0dp"
                android:layout_marginLeft="-45dp"
                android:scaleX="0.6"
                android:scaleY="0.6"
                android:background="@mipmap/lianxiren"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dip"
            android:background="@mipmap/ic_shelf_category_divider" />
        <LinearLayout
            android:id="@+id/contact_main_2"
            android:layout_width="match_parent"
            android:layout_height="45.0dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/contact_main_2_left"
                android:layout_width="80dp"
                android:layout_height="45.0dp"
                android:gravity="center_vertical|center_horizontal">
                <TextView
                    android:id="@+id/contact_main_2_left_text"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="联系人2："
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="#666666"
                    android:textSize="15sp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/contact_main_2_right"
                android:layout_width="match_parent"
                android:layout_height="45.0dp"
                android:gravity="center_vertical|center_horizontal">
                <TextView
                    android:id="@+id/contact_main_2_right_text"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="18332752965"
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="#666666"
                    android:textSize="18sp"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/contact_main_2_image"
                android:layout_width="45dp"
                android:layout_height="45.0dp"
                android:layout_marginLeft="-45dp"
                android:scaleX="0.6"
                android:scaleY="0.6"
                android:background="@mipmap/lianxiren"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dip"
            android:background="@mipmap/ic_shelf_category_divider" />
        <LinearLayout
            android:id="@+id/contact_main_3"
            android:layout_width="match_parent"
            android:layout_height="45.0dp"
            android:orientation="horizontal">
            <LinearLayout
                android:id="@+id/contact_main_3_left"
                android:layout_width="80dp"
                android:layout_height="45.0dp"
                android:gravity="center_vertical|center_horizontal">
                <TextView
                    android:id="@+id/contact_main_3_left_text"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="联系人3："
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="#666666"
                    android:textSize="15sp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/contact_main_3_right"
                android:layout_width="match_parent"
                android:layout_height="45.0dp"
                android:gravity="center_vertical|center_horizontal">
                <TextView
                    android:id="@+id/contact_main_3_right_text"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="18332750357"
                    android:gravity="center_vertical|center_horizontal"
                    android:textColor="#666666"
                    android:textSize="18sp"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/contact_main_3_image"
                android:layout_marginLeft="-45dp"
                android:layout_width="45dp"
                android:layout_height="45.0dp"
                android:scaleX="0.6"
                android:scaleY="0.6"
                android:background="@mipmap/lianxiren"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.1dip"
            android:background="@mipmap/ic_shelf_category_divider" />
    </LinearLayout>
</LinearLayout>