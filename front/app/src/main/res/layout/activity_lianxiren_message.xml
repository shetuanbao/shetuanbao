ckground="@drawable/roundshape">                <LinearLayout                    android:id="@+id/myselfmain_username"                    android:layout_width="match_parent"                    android:layout_height="50dp"                    android:orientation="horizontal"                    android:background="@drawable/wodexiaoyuan02"                    >                    <TextView                        android:id="@+id/myselfmain2_username_text"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="3dp"                        android:padding="10dp"                        android:text="账号："                        android:textColor="#666666"                        android:textSize="14sp"                        />                    <TextView                        android:id="@+id/userfriend_zhanghao_EditView"                        android:layout_width="330dp"                        android:layout_height="wrap_content"                        android:background="@drawable/touming"                        android:text="空"                        android:padding="10dp"                        android:gravity="left|center_vertical"                        android:singleLine="true"                        android:textColor="#666666"                        android:textSize="14sp"                        android:editable="false">                    </TextView>                </LinearLayout>                <LinearLayout                    android:id="@+id/myselfmain_name"                    android:layout_width="match_parent"                    android:layout_height="50dp"                    android:orientation="horizontal"                    android:background="@drawable/wodexiaoyuan02"                    >                    <TextView                        android:id="@+id/myselfmain_name_t"                        andr